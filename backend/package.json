{
  "name": "heirclark-health-backend",
  "version": "2.0.0",
  "description": "Complete Backend API for Heirclark Health App with PostgreSQL, JWT Auth, and 11 AI Agents",
  "main": "server-complete.js",
  "scripts": {
    "build": "echo 'No build step needed - plain JavaScript'",
    "start": "node server-complete.js",
    "start:old": "node server.js",
    "dev": "nodemon server-complete.js",
    "dev:old": "nodemon server.js",
    "test": "jest --coverage",
    "test:e2e": "playwright test",
    "db:init": "psql $DATABASE_URL -f database/schema.sql",
    "db:reset": "psql $DATABASE_URL -c 'DROP SCHEMA public CASCADE; CREATE SCHEMA public;' && npm run db:init"
  },
  "keywords": [
    "health",
    "nutrition",
    "fitness",
    "openai",
    "gpt-4",
    "postgresql",
    "jwt",
    "ai-agents"
  ],
  "author": "Heirclark",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.78.1",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
